setwd("E:\\sample")

data <- read.csv("k1.csv")

head(data)

max_salary <- max(data$salary)
print(max_salary)

salaries <- data$salary
emp_names <- data$name

summary(data$salary)

barplot(
  salaries,
  main = "SALARY",
  ylab = "Salary",
  xlab = "Employee Name",
  col = "blue",
  names.arg = emp_names
)

hist(salaries)
